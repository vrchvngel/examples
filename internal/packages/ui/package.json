{
  "name": "@vercel/examples-ui",
  "version": "1.0.5",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist --copy-files",
    "clean": "rm -rf ./dist",
    "types": "tsc --emitDeclarationOnly"
  },
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./icons": "./dist/icons/index.js",
    "./tailwind": {
      "require": "./dist/tailwind.cjs"
    },
    "./globals.css": "./dist/globals.css"
  },
  "files": [
    "dist/**/*.{js,cjs,d.ts,d.cts,css}"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ],
      "tailwind": [
        "dist/tailwind.d.cts"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ],
        "tailwind": [
          "dist/tailwind.d.cts"
        ]
      }
    }
  },
  "peerDependencies": {
    "next": "*",
    "react": "^17.0.2 || ^18.0.0-0",
    "react-dom": "^17.0.2 || ^18.0.0-0"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.0",
    "clsx": "^1.2.1",
    "sugar-high": "~0.4.6"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@types/react": "^18.0.37",
    "chokidar": "^3.5.3",
    "eslint": "^8.38.0",
    "eslint-config-next": "canary",
    "next": "canary",
    "typescript": "^4.9.5"
  }
}
